    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Nucleo STM32F446",
            "cwd": "${workspaceRoot}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "serverpath": "/usr/bin/openocd",
            "device": "STM32F446x",
            "svdFile": "${workspaceRoot}/STM32-files/STM32F446x.svd",
            "runToMain": true,
            "configFiles": [
                "board/st_nucleo_f4.cfg"
            ]
        }
    ]
